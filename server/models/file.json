{
  "name": "file",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "id": true,
      "type": "string",
      "defaultFn": "uuid"
    },
    "createdAt": {
      "type": "date",
      "defaultFn": "now"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "string",
      "required": true
    },
    "directory": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "directories": {
      "directory": {
        "type": "belongsTo",
        "model": "directory"
      }
    },
    "acls": [
    ],
    "methods": {}
  }
}
